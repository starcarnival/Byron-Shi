import numpy as np
import matplotlib.pyplot as plt
#Q1
np.arange(1,13).reshape(3,4)
#Q2
x = np.arange(1,7)
y = np.arange(5,11)
#Q3
plt.plot(x,y)
plt.show()
#Q4
plt.plot(x,y)
plt.plot(x,y+2)
plt.plot(x,y+4)
plt.plot(x,y+6)
plt.xlabel("X Value")
plt.ylabel("Y Value")
plt.show()
#Q5
plt.subplot(2,2,1)
plt.plot(x,y,"*:b")
plt.subplot(2,2,2)
plt.plot(x,-y,"+-.r")
plt.subplot(2,2,3)
plt.plot(x,y*2,"D-c")
plt.subplot(2,2,4)
plt.plot(y,x+y,"h--m")
plt.show()
